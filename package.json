{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "dotenv -e .env.dev -- tsx watch src/server.ts",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate deploy",
    "migrate:dev:create": "dotenv -e .env.dev -- npx prisma migrate dev --name",
    "migrate:dev:status": "dotenv -e .env.dev -- npx prisma migrate status",
    "migrate:dev:reset": "dotenv -e .env.dev -- npx prisma migrate reset",
    "migrate:dev:rollback": "dotenv -e .env.dev -- npx prisma migrate resolve --rolled-back",
    "migrate:deploy": "npx prisma migrate deploy",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts",
    "fix": "eslint --fix src/**/*.ts",
    "test": "vitest run --dir src/test/unit --test-timeout=6000",
    "test:watch": "vitest --dir src/test/unit --test-timeout=6000",
    "test:coverage": "vitest run --coverage --test-timeout=10000",
    "test:e2e": "vitest run --dir src/test/e2e --test-timeout=10000",
    "test:e2e:watch": "vitest --dir src/test/e2e --test-timeout=10000"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcrypt": "5.0.1",
    "@types/jsonwebtoken": "9.0.4",
    "@types/node": "20.8.7",
    "@types/nodemailer": "6.4.12",
    "@types/supertest": "2.0.16",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.51.0",
    "prisma": "5.4.2",
    "supertest": "6.3.3",
    "tsup": "7.2.0",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.430.0",
    "@fastify/cors": "8.4.0",
    "@prisma/client": "5.4.2",
    "bcrypt": "5.1.1",
    "dotenv": "16.3.1",
    "fastify": "4.24.2",
    "fastify-multer": "2.0.3",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "6.9.6",
    "sharp": "0.32.6",
    "zod": "3.22.4"
  }
